{
    "compilerOptions": {
      "esModuleInterop": true,
      "jsx": "react",
      "lib": [
        "esnext"
      ],
      "module": "esnext",
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "target": "esnext"
    },
    "exclude": [
      "node_modules"
    ]
  }
  